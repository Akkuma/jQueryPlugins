<?xml version="1.0" encoding="UTF-8"?>
<urlset
      xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
            http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
    {% for post in site.posts %}
    <url>
        <loc>http://erikzaadi.github.com/jQueryPlugins{{ post.url }}</loc>
        <lastmod>{{site.time}}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>1.00</priority>
    </url>{% endfor %}{% for plugin in site.plugins %}
    <url>
        <loc>http://erikzaadi.github.com/jQueryPlugins{{ plugin.link }}</loc>
        <lastmod>{{site.time}}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>1.00</priority>
    </url>{% endfor %}
    <url>
        <loc>http://erikzaadi.github.com/jQueryPlugins/posts.html</loc>
        <lastmod>{{site.time}}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>1.00</priority>
    </url>
    <url>
        <loc>http://erikzaadi.github.com/jQueryPlugins/projects.html</loc>
        <lastmod>{{site.time}}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>1.00</priority>
    </url>
</urlset>